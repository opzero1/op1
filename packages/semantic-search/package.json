{
	"name": "@op1/semantic-search",
	"version": "0.3.0",
	"description": "Semantic code search plugin for OpenCode - natural language to code search with embeddings",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "bun build src/index.ts --outfile dist/index.js --target bun --format esm --external sqlite-vec --external @huggingface/transformers",
		"typecheck": "tsc --noEmit",
		"dev": "bun run build --watch"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@opencode-ai/plugin": "^1.0.0",
		"sqlite-vec": "^0.1.7-alpha.2",
		"@huggingface/transformers": "^3.0.0"
	},
	"trustedDependencies": [
		"onnxruntime-node",
		"protobufjs"
	],
	"devDependencies": {
		"@types/bun": "^1.1.0",
		"typescript": "^5.7.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/op1/op1.git",
		"directory": "packages/semantic-search"
	},
	"license": "MIT",
	"keywords": [
		"opencode",
		"plugin",
		"semantic-search",
		"embeddings",
		"vector-search"
	]
}
